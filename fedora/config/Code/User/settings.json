{
  "workbench.colorTheme": "Dracula.min Light",
  "workbench.startupEditor": "newUntitledFile",
  "editor.fontFamily": "JetBrains Mono",
  "path-intellisense.extensionOnImport": true,
  "explorer.confirmDelete": false,
  "python.pythonPath": "/bin/python",
  "git.confirmSync": false,
  "workbench.iconTheme": "material-icon-theme",
  "window.zoomLevel": -2,
  "editor.insertSpaces": false,
  "editor.tabSize": 2,
  "editor.cursorBlinking": "expand",
  "editor.minimap.renderCharacters": false,
  "editor.minimap.enabled": false,
  "editor.wordWrap": "on",
  "editor.snippetSuggestions": "top",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "window.titleBarStyle": "custom",
  "breadcrumbs.enabled": true,
  "explorer.openEditors.visible": 0,
  "workbench.settings.editor": "json",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/node_modules": true
  },
  "git.autofetch": true,
  "editor.cursorSmoothCaretAnimation": true,
  "editor.smoothScrolling": true,
  "explorer.confirmDragAndDrop": false,
  "workbench.settings.useSplitJSON": true,
  "editor.formatOnSave": true,
  "liveshare.featureSet": "insiders",
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "go.useLanguageServer": true,
  "go.autocompleteUnimportedPackages": true,
  "material-icon-theme.files.associations": {
    "*.go": "go_gopher"
  },
  "go.languageServerExperimentalFeatures": {
    "diagnostics": true
  },
  "gopls": {
    "usePlaceholders": true
  },
  "files.eol": "\n",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.tabSize": 4,
    "editor.insertSpaces": true
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "editor.renderWhitespace": "selection",
  "editor.cursorSurroundingLines": 10,
  "editor.formatOnPaste": true,
  "editor.formatOnType": false,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "window.menuBarVisibility": "toggle",
  "C_Cpp.clang_format_path": "/usr/bin/clang-format",
  "editor.fontLigatures": "'ss01', 'ss02', 'ss04', 'ss06', 'zero'",
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/yarn.lock": true
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "C_Cpp.clang_format_style": "Chromium",
  "cSpell.enableFiletypes": [
    "!asciidoc",
    "!c",
    "!cpp",
    "!csharp",
    "!css",
    "!git-commit",
    "!go",
    "!handlebars",
    "!jade",
    "!java",
    "!javascript",
    "!javascriptreact",
    "!less",
    "!php",
    "!pug",
    "!python",
    "!rust",
    "!scss",
    "!typescript",
    "!typescriptreact",
    "csv",
    "toml"
  ],
  "workbench.preferredDarkColorTheme": "Material Theme Darker",
  "workbench.preferredLightColorTheme": "Dracula.min Light",
  "editor.renameOnType": true,
  "extensions.ignoreRecommendations": false,
  "java.configuration.checkProjectSettingsExclusions": false,
  "cSpell.language": "en",
  "java.semanticHighlighting.enabled": true
}
