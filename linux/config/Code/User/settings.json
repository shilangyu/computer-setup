{
  "workbench.startupEditor": "newUntitledFile",
  "editor.fontFamily": "JetBrainsMono Nerd Font",
  "path-intellisense.extensionOnImport": true,
  "explorer.confirmDelete": false,
  "git.confirmSync": false,
  "editor.insertSpaces": false,
  "editor.tabSize": 2,
  "editor.cursorBlinking": "expand",
  "editor.minimap.renderCharacters": false,
  "editor.minimap.enabled": false,
  "editor.wordWrap": "on",
  "editor.snippetSuggestions": "top",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "window.titleBarStyle": "custom",
  "breadcrumbs.enabled": true,
  "explorer.openEditors.visible": 0,
  "workbench.settings.editor": "json",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "git.autofetch": true,
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.smoothScrolling": true,
  "explorer.confirmDragAndDrop": false,
  "workbench.settings.useSplitJSON": true,
  "editor.formatOnSave": true,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "go.useLanguageServer": true,
  "go.autocompleteUnimportedPackages": true,
  "material-icon-theme.files.associations": {
    "*.go": "go_gopher",
    "*.sage": "python"
  },
  "go.languageServerExperimentalFeatures": {
    "diagnostics": true
  },
  "gopls": {
    "usePlaceholders": true
  },
  "files.eol": "\n",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[github-actions-workflow]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[python]": {
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.defaultFormatter": "ms-python.python"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit"
  },
  "[dart]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit",
      "source.fixAll": "explicit"
    }
  },
  "editor.renderWhitespace": "selection",
  "editor.cursorSurroundingLines": 10,
  "editor.formatOnType": false,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "window.menuBarVisibility": "toggle",
  "editor.fontLigatures": true,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/yarn.lock": true
  },
  "editor.formatOnPaste": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "cSpell.enableFiletypes": [
    "!asciidoc",
    "!c",
    "!cpp",
    "!csharp",
    "!css",
    "!dart",
    "!git-commit",
    "!go",
    "!handlebars",
    "!jade",
    "!java",
    "!javascript",
    "!javascriptreact",
    "!less",
    "!php",
    "!pug",
    "!python",
    "!rust",
    "!scala",
    "!scss",
    "!typescript",
    "!typescriptreact",
    "csv",
    "toml"
  ],
  "workbench.preferredDarkColorTheme": "Dracula.min",
  "workbench.preferredLightColorTheme": "Dracula.min Light",
  "editor.linkedEditing": true,
  "extensions.ignoreRecommendations": false,
  "cSpell.language": "fr,en",
  "julia.enableTelemetry": false,
  "window.autoDetectColorScheme": true,
  "workbench.colorTheme": "Dracula.min",
  "workbench.iconTheme": "material-icon-theme",
  "dart.warnWhenEditingFilesOutsideWorkspace": false,
  "C_Cpp.default.cppStandard": "c++20",
  "terminal.integrated.commandsToSkipShell": ["language-julia.interrupt"],
  "C_Cpp.clang_format_fallbackStyle": "Chromium",
  "[zig]": {
    "editor.defaultFormatter": "tiehuis.zig"
  },
  "liveshare.authenticationProvider": "GitHub",
  "dart.updateImportsOnRename": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "workbench.productIconTheme": "fluent-icons",
  "dart.runPubGetOnPubspecChanges": "never",
  "sql-formatter.uppercase": true,
  "csharp.referencesCodeLens.enabled": false,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "security.workspace.trust.untrustedFiles": "open",
  "julia.symbolCacheDownload": true,
  "editor.bracketPairColorization.enabled": true,
  "latex-workshop.view.pdf.viewer": "tab",
  "latex-workshop.latex.tools": [
    {
      "name": "latexmk",
      "command": "latexmk",
      "args": [
        "-shell-escape",
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-pdf",
        "-outdir=%OUTDIR%",
        "%DOC%"
      ],
      "env": {}
    }
  ],
  "redhat.telemetry.enabled": false,
  "dart.debugExternalPackageLibraries": true,
  "dart.debugSdkLibraries": true,
  "omnisharp.enableRoslynAnalyzers": true,
  "omnisharp.enableEditorConfigSupport": true,
  "debug.onTaskErrors": "abort",
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "*.dart": "$(capture).g.dart,$(capture).freezed.dart"
  },
  "git.autoRepositoryDetection": false,
  "rust-analyzer.inlayHints.closingBraceHints.enable": false,
  "rust-analyzer.inlayHints.parameterHints.enable": false,
  "mergeEditor.diffAlgorithm": "advanced",
  "editor.stickyScroll.enabled": true,
  "rust-analyzer.inlayHints.lifetimeElisionHints.enable": "skip_trivial",
  "[julia]": {
    "editor.unicodeHighlight.ambiguousCharacters": false
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "rust-analyzer.check.command": "clippy",
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "jupyter.widgetScriptSources": ["jsdelivr.com", "unpkg.com"],
  "git.openRepositoryInParentFolders": "always",
  "notebook.formatOnSave.enabled": true,
  "dotnet.codeLens.enableReferencesCodeLens": false,
  "[csharp]": {
    "editor.defaultFormatter": "csharpier.csharpier-vscode"
  },
  "window.commandCenter": false,
  "workbench.layoutControl.enabled": false,
  "files.watcherExclude": {
    "**/.bloop": true,
    "**/.metals": true,
    "**/.ammonite": true
  },
  "diffEditor.useInlineViewWhenSpaceIsLimited": false,
  "workbench.activityBar.location": "hidden",
  "files.associations": {
    "*.sage": "python",
    "*.arb": "json"
  },
  "python.analysis.typeCheckingMode": "strict"
}
